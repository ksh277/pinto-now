{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --force",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "functions": {
    "src/app/**/*.js": {
      "memory": 1024
    },
    "src/app/api/**/*.js": {
      "memory": 512
    }
  },
  "ignoreCommand": "npx turbo-ignore",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}